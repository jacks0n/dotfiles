{
  "codeLens.enable": true,
  "eslint.formatterPriority": 999,
  "prettier.formatterPriority": 999,
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": true,
    "highlight": "CocFloating",
    "shadow": false,
  },
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "▲",
  "diagnostic.hintSign": "ﯦ",
  "diagnostic.infoSign": "",
  "suggest.completionItemKindLabels": {
    "class": "",
    "color": "",
    "constant": "",
    "default": "",
    "enum": "",
    "enumMember": "",
    "event": "鬒",
    "field": "m",
    "file": "",
    "folder": "",
    "function": "F",
    "interface": "",
    "keyword": "",
    "method": "f",
    "module": "",
    "operator": "Ψ",
    "property": "",
    "reference": "r",
    "snippet": "",
    "struct": "פּ",
    "text": "",
    "typeParameter": "",
    "unit": "",
    "value": "",
    "variable": ""
  },
  "suggest.removeDuplicateItems": true,
  "list.floatPreview": true,
  "semanticTokens.enable": true,
  "suggest.pumFloatConfig": {
    "border": true,
    "rounded": true,
    "borderhighlight": "CocFloating",
    "winblend": 10,
    "shadow": false
  },
  "suggest.noselect": false,
  "suggest.floatConfig": {
    "border": true,
    "rounded": true,
    "borderhighlight": "CocFloating",
    "maxWidth": 120,
    "winblend": 5,
    "shadow": false
  },
  "signature.floatConfig": {
    "border": true,
    "rounded": true,
    "borderhighlight": "CocFloating",
    "maxWidth": 100,
    "winblend": 15,
    "maxHeight": 30,
    "shadow": false
  },
  "hover.floatConfig": {
    "maxHeight": 30,
    "border": true,
    "shadow": false,
    "borderhighlight": "CocFloating",
    "rounded": true,
    "maxWidth": 120,
    "winblend": 10
  },
  "intelephense.files.associations": [
    "*.php",
    "*.inc",
    "*.module",
    "*.phtml"
  ],
  "intelephense.files.exclude": [
    "**/.git/**",
    "**/.svn/**",
    "**/.hg/**",
    "**/node_modules/**",
    "**/vendor/**"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "javascript.format.enable": false,
  "javascript.format.semicolons": "insert",
  "javascript.suggest.includeAutomaticOptionalChainCompletions": true,
  "typescript.suggest.includeAutomaticOptionalChainCompletions": true,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.showUnused": true,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.suggestionActions.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "tsserver.experimental.enableProjectDiagnostics": true,
  "tsserver.implicitProjectConfig.checkJs": true,
  "tsserver.trace.server": "verbose",
  "typescript.format.semicolons": "insert",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.showUnused": true,
  "typescript.suggest.autoImports": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.suggest.enabled": true,
  "typescript.suggestionActions.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "yaml.completion": true,
  "yaml.format.bracketSpacing": true,
  "yaml.format.singleQuote": true,
  "yaml.hover": true,
  "yaml.validate": true,
}
